  "compatibility_flags": ["nodejs_compat"],
  "main": "workers/email-receiver.ts",
  "d1_databases": [
    {
      "binding": "DB",
      "migrations_dir": "drizzle",
      "database_name": "moemail",
      "database_id": "${DATABASE_ID}"
    }
  ]
}
